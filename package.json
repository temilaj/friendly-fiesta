{
  "name": "smartassist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./bin/www",
    "server": "DEBUG=smartassist:* npm start",
    "server:watch": "nodemon DEBUG=smartassist:* npm start --ignore 'public/*'",
    "build": "./node_modules/.bin/webpack --config webpack.config.dev.js",
    "build:production": "webpack --mode production",
    "client": "webpack-dev-server --config webpack.config.dev.js",
    "dev": "concurrently \"npm run server:watch\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.8.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "recharts": "^1.6.2",
    "react-vis": "^1.11.7",
    "sequelize": "^5.8.10"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "css-loader": "0.28.9",
    "extract-text-webpack-plugin": "3.0.2",
    "node-sass": "4.13.1",
    "react-hot-loader": "3.1.3",
    "sass-loader": "6.0.6",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1",
    "path": "0.12.7",
    "concurrently": "4.0.0",
    "fs": "0.0.2"
  }
}
